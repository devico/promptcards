<div id="flickr">
  <%= f.button :submit, t('dashboard.cards.downflickr'), class: 'btn btn-default' %>  
  <div id="find">
    <div class="form-group">
      <%= f.input :tags,  as: :text, label: t('dashboard.cards.tags'),
                  input_html: { class: 'form-control'} %>
    </div>
    <%= f.button :submit, t('dashboard.cards.findflickr'), class: 'btn btn-default' %>
    <div id="photos" />
  </div>
</div>
